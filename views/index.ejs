              <script type="text/javascript">
                var cur_x = 0;
                var cur_y = 0;
                
                var basicTimeline = anime.timeline();
                function moveDirN(dir, n)
                {
                  //up down left right 0 1 2 3 
                  if(dir == 0)
                  {
                    for(var i = 0; i < n; i++)
                      basicTimeline.add({
                        targets: '#maze_timeline .img1',
                        translateX: 50*(cur_x),
                        translateY: 50*(cur_y--),
                        easing: 'easeOutExpo'
                
                      });
                  }
                  else if(dir == 1)
                  {
                    for(var i = 0; i < n; i++)
                      basicTimeline.add({
                        targets: '#maze_timeline .img1',
                        translateX: 50*(cur_x),
                        translateY: 50*(cur_y++),
                        easing: 'easeOutExpo'
                
                      });
                  }
                  else if(dir == 2)
                  {
                    for(var i = 0; i < n; i++)
                      basicTimeline.add({
                        targets: '#maze_timeline .img1',
                        translateX: 50*(cur_x--),
                        translateY: 50*(cur_y),
                        easing: 'easeOutExpo'
                      });
                  }
                  else if(dir == 3)
                  {
                    for(var i = 0; i < n; i++)
                      basicTimeline.add({
                        targets: '#maze_timeline .img1',
                        translateX: 50*(cur_x++),
                        translateY: 50*(cur_y),
                        easing: 'easeOutExpo'
                      });
                  }
                }
                var hint = document.getElementsByName('hint')[0].id;
                document.getElementById('playButton').onclick = function(e){
                moveDirN(3, 5);
                moveDirN(1, 3);
                moveDirN(2, 3);
                moveDirN(0, 3);
                };
